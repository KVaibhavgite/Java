class Primereturn
{
public void printreturn(int n)
{
int flag=1;
for(int i=2;i<=n;i++)
{
if(n%i==0)
{
flag=2;
break;
}
}
if(flag==1)
{
return n;
}
else
{
return n+"not a Prime";
}
}
