/*WAJP to count even and Odd number between 100and 500.*/
class EvenOrOddReturn11
{
public static boolean evenOrOdd(int num)
{
if(num%2==0)
{
return true;
}
else
{
return false;
}
}
public static void main(String[]args)
{
int count=0;
for(int i=100;i<=500;i++)
	{
if(evenOrOdd(i))
	{
	count++;
	System.out.println("Total count even number="+count);
	}
	else
	{
	        count++;
		System.out.println("Total count odd number="+count);
	}
}
System.out.println("Total count even number="+count);
System.out.println("Total count odd number="+count);
}
}