/*WAJP to check  wheather a number is Xylem or phloem.*/
class Number
{
public static void main(String[]args)
{
int n=34326;
int temp=n,outsum=0,innersum=0;
while(temp!=0)
{
if(temp==n||temp<10)
outsum=outsum+temp%10;
else
innersum=innersum+temp%10;
temp=temp/10;
}
if(outsum==innersum)
{
System.out.println("it is Xylem");
}
else
{
System.out.println("it is phlom no.");
}
}
}


