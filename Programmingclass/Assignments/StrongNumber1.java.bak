/*WAJP to check wheather a number is  stong number or not.*/
class StrongNumber1
{
public static void main(String[]args)
{
	int n=145;
int sum=0;
int n1=n;
while(n!=0)
{
int d=n%10;
sum=sum+fac(d);
n=n/10;
}
if(sum==n1)
{
System.out.println("Strong number:"+n);
}
else
{
System.out.println("not aStrong number:"+n);
}
}
public static int fac(int n)
{
int f=1;
for(int i=1;i<=n;i++)
{
f=f*i;
}
return f;
}
}