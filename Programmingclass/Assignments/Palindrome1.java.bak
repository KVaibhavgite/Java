/*design a method can accept a number and return the revers of a number.*/
class Palindrome1
{
public static int printnum(int num)
{
int temp=num;
int rev=0;
while(temp!=0)
{
rev=rev*10+temp%10;
temp=temp/10;
}
if(num==rev)
{
System.out.println(num+"  is palindrome");
}
else
{
System.out.println(num+"  is  Not palindrome");
}
return rev;
}
public static void main(String[]args)
{
Palindrome a=new Palindrome();
printnum(1234);
}
}