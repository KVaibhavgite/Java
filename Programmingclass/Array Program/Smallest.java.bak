/*WAJP to find out the smallest element of an array.
int [] a={6,2,3,4,7,8,11}
o/p=2
*/
class Smallest
{
public static void main(String[]args)
{
int []a={6,2,3,4,7,8,11};
int smallest=a[0];
for(int i=0;i<a.length;i++)
{
if(a[i]<smallest)
{
smallest=a[i];
}
}
System.out.println("Smallest is :"+smallest);//11
}
}
