/*WAJP to find out the largest element of an array.
int [] a={6,2,3,4,7,8,11}
o/p=11
*/
class Largest
{
public static void main(String[]args)
{
int []a={6,2,3,4,7,8,11};
int largest=a[0];
for(int i=0;i<a.length;i++)
{
if(a[i]>largest)
{
largest=a[i];
}
}
System.out.println("largest is :"+largest);
}
}
